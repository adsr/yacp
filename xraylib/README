  /usr/include/xraylib/xraylib-parser.h
  /usr/include/xraylib/xraylib-radionuclides.h
  /usr/include/xraylib/xraylib-shells.h
  /usr/include/xraylib/xraylib.h
  /usr/lib/libxrl.dll.a
