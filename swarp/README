  /usr/share/doc/swarp/README.md
  /usr/share/doc/swarp/THANKS
  /usr/share/doc/swarp/TODO
  /usr/share/man/man1/swarp.1.gz
  /usr/share/man/manx/swarp.x.gz
  /usr/share/swarp/swarp.xsl

