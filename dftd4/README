  /usr/include/dftd4/dftd4.h
  /usr/include/dftd4/dftd4.mod
  /usr/include/dftd4/dfuncpar.mod
  /usr/include/dftd4/dispersion_calculator.mod
  /usr/include/dftd4/eeq_model.mod
  /usr/include/dftd4/geometry_reader.mod
  /usr/include/dftd4/mctc_constants.mod
  /usr/include/dftd4/mctc_econv.mod
  /usr/include/dftd4/mctc_environment.mod
  /usr/include/dftd4/mctc_global.mod
  /usr/include/dftd4/mctc_param.mod
  /usr/include/dftd4/mctc_readin.mod
  /usr/include/dftd4/mctc_strings.mod
  /usr/include/dftd4/mctc_systools.mod
